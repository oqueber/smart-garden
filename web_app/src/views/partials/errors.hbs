{{#if error }}
   <div onclick="notificationDelete(this)" class="alert show showAlert">
      <span class="fas fa-exclamation-circle"></span>
      <span class="msg">{{error}}</span>
      <div  class="close-btn">
        <i class="material-icons">cancel</i>
      </div>
  </div>
{{/if}}

{{#each errors}}
  <div onclick="notificationDelete(this)" class="alert show showAlert">
      <span class="fas fa-exclamation-circle"></span>
      <span class="msg">{{text}}</span>
      <div  class="close-btn">
        <i class="material-icons">cancel</i>
      </div>
  </div>
{{/each}}